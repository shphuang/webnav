{
  "version": "0.2.0",
  "configurations": [
    {
      "request": "launch",
      "name": "Launch Program",
      "type": "node",
      "program": "${workspaceFolder}/main.ts",
      "cwd": "${workspaceFolder}",
      "env": {},
      "runtimeExecutable": "deno.exe",
      "runtimeArgs": [
        "run",
        "--inspect-wait",
        "--allow-all",
        "--watch"
      ],
      "attachSimplePort": 9229
    }
  ]
}
